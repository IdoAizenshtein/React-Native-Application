{
  "name": "BiziboxUI",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "ios": "react-native run-ios",
    "prettier:fix": "npx prettier --write .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "cat config/core.ts > node_modules/react-native-reanimated/src/reanimated2/core.ts && cat config/build.gradle > node_modules/react-native-background-upload/android/build.gradle && cat config/gradle.properties > node_modules/react-native-webview/android/gradle.properties",
    "tsc": "yarn tsc -p ./tsconfig.json",
    "lint": "yarn lint:ts && yarn lint:style",
    "lint:ts": "tsc --noEmit",
    "lint:style": "eslint \"**/*.{js,ts,jsx,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,ts,jsx,tsx}\" --quiet --fix",
    "postinstallOld": "cat config/build.gradle > node_modules/react-native-background-upload/android/build.gradle && cat config/gradle.properties > node_modules/react-native-webview/android/gradle.properties",
    "podinstall": "npx pod-install",
    "create:android:apk:dev": "cd android/ && ./gradlew assembleDevRelease && cd ..",
    "create:android:apk:prod": "cd android/ && ./gradlew assembleProdRelease && cd ..",
    "create:android:apk:prod:bundle": "cd android/ && ./gradlew bundleRelease && cd ..",
    "clean": "react-native-clean-project",
    "reinstall-pods": "cd ios && rm -rf Pods/ Podfile.lock && pod install --repo-update && cd ..",
    "reinstall-bundle-pods": "cd ios && rm -rf Pods/ Podfile.lock && bundle install && bundle exec pod install && cd ..",
    "clean-droid-build": "cd android && ./gradlew app:clean && cd .."
  },
